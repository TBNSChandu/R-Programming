data10 <- read.csv("question10.csv")
net1 <- neuralnet(y ~ ., data = data10, hidden = 1)
plot(net1)
net2 <- neuralnet(y ~ ., data = data10, hidden = 2)
plot(net2)
net3 <- neuralnet(y ~ ., data = data10, hidden = 3)
plot(net3)
compute(net3, data.frame(x1=0.1,x2=0.5,x3=0.5,x4=0.7))
